    "## Gemeinsame Definitionen für die Visualisierungen"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "T = 3\n",
    "L = 1.5\n",
    "t = np.linspace(-L*T, L*T, 6000)\n",
    "\n",
    "# Anzahl der beruecksichtigten Frequenzkomponenten\n",
    "K_max = 50\n",
    "k_c = np.arange(K_max)\n",
    "# Kosinus-Kern als Matrix (fuer Fouriersumme)\n",
    "Cos = np.cos(2*np.pi/T*(t[:, None] @ k_c[None ,:]))\n",
    "Cos[:, 0] = 0.5"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Funktion, um eine Fouriersummeaus gegebenen Koeffizienten \n",
    "# sowie das Referenzsignal zu plotten\n",
    "def plot_fourier_series(a_k, x_ref, t, T):\n",
    "    # Fouriersumme als Matrixprodukt\n",
    "    x_c = Cos[:, :a_k.shape[0]] @ a_k[:, None]\n",
    "    \n",
    "    plt.plot(t/T, x_ref, alpha=0.5, lw=2)\n",
    "    plt.plot(t/T, x_c, 'r');\n",
    "    plt.grid(True);\n",
    "    plt.ylabel('$x(t)$');\n",
    "    plt.xlabel('$t/T$');\n",
    "    plt.ylim((-0.2, 1.2))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "# Funktion, um einen gegebenen Impuls zu wiederholen\n",
    "# (signal muss eine zeitabhaengige Funktion sein)\n",
    "def rep_pulse(signal, t, T):\n",
    "    l = np.arange(-np.floor(L), np.floor(L)+1)\n",
    "    return np.sum(signal(t[:,None] - l[None, :]*T), 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Funktion, um Fouriersumme zusammen mit wiederholtem Impuls zu plotten\n",
    "def plot_signal(a_k, ref_pulse, t, T):\n",
    "    x_ref = rep_pulse(ref_pulse, t , T)\n",
    "    plot_fourier_series(a_k, x_ref, t, T)"
   ]
